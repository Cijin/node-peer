{
  "name": "node-peer",
  "version": "1.0.0",
  "description": "a service to send and recieve data between two or more people",
  "main": "index.ts",
  "scripts": {
    "postinstall": "git config core.hooksPath ./Scripts",
    "watch": "tsc --skipLibCheck --watch",
    "dev": "nodemon --quiet --watch dist --exec 'clear;node dist/src/index.js'",
    "lint": "eslint . --ext .js --ext .ts",
    "autofix": "eslint . --ext .js --ext .ts  --fix",
    "type-check": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cijin/node-peer.git"
  },
  "author": "Cijin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cijin/node-peer/issues"
  },
  "homepage": "https://github.com/Cijin/node-peer#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
